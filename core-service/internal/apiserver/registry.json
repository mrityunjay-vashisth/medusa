{
  "core": {
    "v1": {
      "/tenants/{action}": {
        "methods": ["POST", "GET"],
        "subhandler": "TenantHandler"
      },
      "/tenants/{action}/{id}":{
        "methods": ["GET", "PATCH"],
        "subhandler": "TenantHandler"
      },
      "/users": {
        "methods": ["POST"],
        "subhandler": "UserHandler"
      },
      "/users/{id}": {
        "methods": ["GET"],
        "subhandler": "UserHandler"
      },
      "/auth/{subpath}": {
        "methods": ["POST"],
        "subhandler": "AuthHandler"
      }
    }
  }
}